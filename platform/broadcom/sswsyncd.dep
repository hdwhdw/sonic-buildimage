#DPKG FRK
DPATH       := $(PLATFORM_PATH)/sswsyncd
DEP_FILES   := $(SONIC_COMMON_FILES_LIST) platform/broadcom/sswsyncd.mk platform/broadcom/sswsyncd.dep
DEP_FILES   += $(shell git ls-files $(DPATH))

$(SSWSYNCD_BCMCMD)_CACHE_MODE  := GIT_CONTENT_SHA
$(SSWSYNCD_BCMCMD)_DEP_FLAGS   := $(SONIC_COMMON_FLAGS_LIST)
$(SSWSYNCD_BCMCMD)_DEP_FILES   := $(DEP_FILES)

$(SSWSYNCD_DSSERVE)_CACHE_MODE  := GIT_CONTENT_SHA
$(SSWSYNCD_DSSERVE)_DEP_FLAGS   := $(SONIC_COMMON_FLAGS_LIST)
$(SSWSYNCD_DSSERVE)_DEP_FILES   := $(DEP_FILES)
